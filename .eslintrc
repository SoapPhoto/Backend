{
  "extends": [
    "yiiu",
    "yiiu/typescript"
  ],
  "rules": {
    "no-undef": 0,
    // "no-unused-locals": 1,
    "no-plusplus": "off",
    "max-len": [
      "error",
      160
    ],
    "@typescript-eslint/explicit-module-boundary-types": ["off"],
    "@typescript-eslint/indent": ["error", 2],
    "import/extensions": "off",
    "no-const-assign": "off"
  }
}
