{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "baseUrl": "./",
    "types": [
      "webpack-env",
      "node"
    ],
    "lib": [
      "es6",
      "dom",
      "es2015",
      "es2017"
    ],
    "typeRoots": [ "../node_modules/@types" ],
    "paths": {
      "@server/*": [
        "src/*"
      ],
      "@typings/*": [
        "typings/*"
      ],
      "@common/*": [
        "common/*"
      ]
    }
  }
}
